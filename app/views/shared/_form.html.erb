<%= form_with model: travel, html: { data: { action: "submit->loader#loading" } } do |f| %>
  <%= f.label :budget %>
  <%= f.text_field :budget %>
  <%= f.label :type_of_travelers %>
  <%= f.text_field :type_of_travelers %>
  <%= f.label :type_of_destination %>
  <%= f.text_field :type_of_destination %>
  <%= f.label :duration %>
  <%= f.text_field :duration %>
  <%= f.submit %>
<% end %>
